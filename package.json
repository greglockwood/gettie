{
  "name": "gettie",
  "private": true,
  "description": "JS Object props coverage",
  "scripts": {
    "bootstrap": "lerna init",
    "build": "lerna run build",
    "build:prod": "lerna run build:prod",
    "fix": "npm run lint -- --fix",
    "lint": "eslint . --ext '.js' --cache",
    "test": "npm run lint && npm run test:jest",
    "test:jest": "jest",
    "test:update": "jest -u",
    "test:coverage": "jest --coverage",
    "test:watch": "npm run test:jest -- --watch",
    "prepublish": "lerna run prepublish",
    "postinstall": "npm run bootstrap"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/smelukov/gettie"
  },
  "author": {
    "name": "Serget Melyukov",
    "url": "https://twitter.com/smelukov"
  },
  "bugs": {
    "url": "https://github.com/smelukov/gettie/issues"
  },
  "homepage": "https://github.com/smelukov/gettie",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix --cache"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-proposal-private-methods": "^7.12.1",
    "@babel/plugin-syntax-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lerna": "^3.22.1",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "webpack": "^5.5.0",
    "webpack-cli": "^4.2.0"
  }
}
